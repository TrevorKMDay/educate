citHeader("To cite educate in publications use:")

citEntry(entry = "Misc",
  title        = "{educate}: Miscellaneous R Functions for Educational Statistics",
  author       = personList(as.person("Andrew Zieffler")),
  year         = "2019",
  url          = "https://github.com/zief0002/educate",

  textVersion  =
  paste("Zieffler, A. (2019).",
        "educate: Miscellaneous R functions for educational statistics.",
        "https://github.com/zief0002/educate.")
)
